      "types": "./src/types/V8/index.ts",
      "import": "./dist/types/V8/index.js"
    },
    "./types/V7": {
      "types": "./src/types/V7/index.ts",
      "import": "./dist/types/V7/index.js"
    },
    "./types/domain": {
      "types": "./src/types/domain/index.ts",
      "import": "./dist/types/domain/index.js"
    },
    "./types/domain/account": {
      "types": "./src/types/domain/account.ts",
      "import": "./dist/types/domain/account.js"
    },
    "./types/V9/armories": {
      "types": "./src/types/V9/armories.ts",
      "import": "./dist/types/V9/armories.js"
    },
    "./types/V9/auctions": {
      "types": "./src/types/V9/auctions.ts",
      "import": "./dist/types/V9/auctions.js"
    },
    "./types/V9/characters": {
      "types": "./src/types/V9/characters.ts",
      "import": "./dist/types/V9/characters.js"
    },
    "./types/V9/gamecontents": {
      "types": "./src/types/V9/gamecontents.ts",
      "import": "./dist/types/V9/gamecontents.js"
