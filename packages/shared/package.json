{
  "name": "@lostark/shared",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./dist/index.js"
    },
    "./types": {
      "types": "./src/types/latest/index.ts",
      "import": "./dist/types/latest/index.js"
    },
    "./types/latest": {
      "types": "./src/types/latest/index.ts",
      "import": "./dist/types/latest/index.js"
    },
