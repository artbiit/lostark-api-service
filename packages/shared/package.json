{
  "name": "@lostark/shared",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    },
    "./types": {
      "types": "./src/types/latest/index.ts",
      "import": "./src/types/latest/index.ts"
    },
    "./types/latest": {
      "types": "./src/types/latest/index.ts",
      "import": "./src/types/latest/index.ts"
    },
    "./types/V9": {
      "types": "./src/types/V9/index.ts",
      "import": "./src/types/V9/index.ts"
    },
    "./types/V8": {
      "types": "./src/types/V8/index.ts",
      "import": "./src/types/V8/index.ts"
    },
    "./types/V7": {
      "types": "./src/types/V7/index.ts",
      "import": "./src/types/V7/index.ts"
    },
    "./types/domain": {
      "types": "./src/types/domain/index.ts",
      "import": "./src/types/domain/index.ts"
    },
    "./types/domain/account": {
      "types": "./src/types/domain/account.ts",
      "import": "./src/types/domain/account.ts"
    },
    "./config": {
      "types": "./src/config/index.ts",
      "import": "./src/config/index.ts"
    },
    "./utils": {
      "types": "./src/utils/index.ts",
      "import": "./src/utils/index.ts"
    },
    "./db": {
      "types": "./src/db/index.ts",
      "import": "./src/db/index.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rimraf dist",
    "typecheck": "tsc --noEmit",
    "test": "tsx --test ../../tests/unit/shared/**/*.test.ts",
    "test:watch": "tsx --test --watch ../../tests/unit/shared/**/*.test.ts"
  },
  "dependencies": {
    "@types/redis": "^4.0.11",
    "dotenv": "^17.2.1",
    "mysql2": "^3.14.3",
    "pino": "^8.0.0",
    "pino-pretty": "^10.0.0",
    "redis": "^5.8.2",
    "zod": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^24.0.0",
    "typescript": "^5.9.2"
  }
}
