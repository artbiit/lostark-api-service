    "./types/V9/markets": {
      "types": "./src/types/V9/markets.ts",
      "import": "./dist/types/V9/markets.js"
    },
    "./types/V9/news": {
      "types": "./src/types/V9/news.ts",
      "import": "./dist/types/V9/news.js"
    },
    "./config": {
      "types": "./src/config/index.ts",
      "import": "./dist/config/index.js"
    },
    "./config/env": {
      "types": "./src/config/env.ts",
      "import": "./dist/config/env.js"
    },
    "./utils": {
      "types": "./src/utils/index.ts",
      "import": "./src/utils/index.ts"
    },
    "./db": {
      "types": "./src/db/index.ts",
      "import": "./dist/db/index.js"
    },
    "./db/mysql": {
      "types": "./src/db/mysql.ts",
      "import": "./dist/db/mysql.js"
    },
    "./db/redis": {
      "types": "./src/db/redis.ts",
      "import": "./dist/db/redis.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rimraf dist",
    "typecheck": "tsc --noEmit",
    "test": "tsx --test ../../tests/unit/shared/**/*.test.ts",
    "test:watch": "tsx --test --watch ../../tests/unit/shared/**/*.test.ts"
  },
  "dependencies": {
    "@types/redis": "^4.0.11",
    "dotenv": "^17.2.1",
    "mysql2": "^3.14.3",
    "pino": "^8.0.0",
    "pino-pretty": "^10.0.0",
    "redis": "^5.8.2",
    "zod": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^24.0.0",
    "typescript": "^5.9.2"
  }
